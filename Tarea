SELECT 
/*No sabia si eran los dos o Pias o Country ya que lo tengo en espa単ol 
y el otro currencies esta en ingles.*/
    f.Pais AS Country, 
    f.Producto, 
    f.PrecioVenta, 
    f.PrecioManufactura, 
    c.exchange_rate, 
    f.A単o, 
    f.NombreMes
FROM financialsample f
JOIN currencies c ON f.Pais = c.country
WHERE f.Pais = 'MEXICO' 
AND f.A単o = 2014 
AND f.NumeroMes IN (1, 2) 
ORDER BY f.A単o, f.NumeroMes, f.Producto;

    
